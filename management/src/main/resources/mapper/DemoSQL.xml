<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.management.management.mapper.BoardMapper">

    <select id="select" resultType="com.management.management.DTO.Test">
        SELECT *
        FROM board
    </select>

    <insert id="insert">
        INSERT INTO board(
            id
            ,name
        ) VALUES(
            #{id}
            ,#{name}
        )
    </insert>
</mapper>